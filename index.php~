		<script type="text/javascript">
			var slide = 1;
		var slideshow =	setInterval(function(){
				$('#slide'+slide).fadeOut(1500);
				$('#subtitle'+slide).hide();
				if(slide < 4){
					slide = slide + 1;
				} else {
					slide = 1;
				}
				$('#slide'+slide).fadeIn(1500);
				$('#subtitle'+slide).show();
				$('.control').removeClass('active');
				$('#control'+slide).addClass('active');
			},5000);
			$(document).ready(function(){
			$('.control').click(function(){
					clearInterval(slideshow);
					$('.slides').hide();
					$('.page-head span').hide();
					slide = $(this).attr('name');
					$('#slide'+slide).show();
					$('#subtitle'+slide).show();					
					$('.control').removeClass('active');
					$('#control'+slide).addClass('active');					
			});

			
			});
			
		</script>
		
		
