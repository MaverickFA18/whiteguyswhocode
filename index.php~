					<script type="text/javascript">
					$('.trigger-donate').click(function(){
						$('.btn-cta').trigger('click');
					});
					var $row = 1;
					$('.why-viewport a.arrow-right').click(function(){

						if($row < 8){
					
						$row++;
						
						if($row >= 8){
							$('.why-viewport a.arrow-right').removeClass('enable-arrow');							
						}
						
						console.log($row);
						
						$('.why-viewport a.arrow-left').addClass('enable-arrow');
						$('.fact-row').fadeOut(500,function(){
							setTimeout(function(){
								$('#row'+$row).fadeIn(1000);
							},500);						
						});
						
						}
					});
					$('.why-viewport a.arrow-left').click(function(){
					
						if($row > 1){
					
						$row = $row-1;
						if($row <= 1){
							$('.why-viewport a.arrow-left').removeClass('enable-arrow');							
						}
						$('.why-viewport a.arrow-right').addClass('enable-arrow');
						$('.fact-row').fadeOut(500,function(){
						
							setTimeout(function(){
								$('#row'+$row).fadeIn(1000);						
							},500);
						
						});
						
						}
					});					
			$(window).resize(function(){
				$('body').css({width: $(window).width()+'px'});			
			});
			
			$(document).ready(function(){
				$('body').css({width: $(window).width()+'px'});						
			});
					
			var navcollapse = true;
			$('.btn-expand').click(function(){
				if(navcollapse){
					$('.nav-collapse').animate({marginLeft:'0px'},500);
					$('body').animate({left:'300px'},500);
					navcollapse = false;
				} else {
					$('.nav-collapse').animate({marginLeft:'-300px'},500);
					$('body').animate({left:'0px'},500);				
					navcollapse = true;
				}
			});					
				</script>						
